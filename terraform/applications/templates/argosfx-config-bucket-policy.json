{
    "Version": "2012-10-17",
    "Statement": [   
      {
        "Sid": "RequireKMSEncryption",
        "Effect": "Deny",
        "Principal": "*",
        "Action": "s3:PutObject",
        "Resource": "arn:aws:s3:::${bucket_name}/*",
        "Condition": {
            "StringNotEquals": {
                "s3:x-amz-server-side-encryption-aws-kms-key-id": "${data_kms_arn}"
            }
        }
    }
    ]
  }